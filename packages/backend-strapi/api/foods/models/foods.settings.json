{
  "kind": "collectionType",
  "collectionName": "foods",
  "info": {
    "name": "Foods"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "foodname": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal"
    },
    "available": {
      "type": "integer"
    },
    "img": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "appetizer",
        "hotdish",
        "colddish",
        "grill",
        "soup",
        "dessert"
      ]
    }
  }
}
